{
  "dependencies": {
    "@abstract-foundation/agw-client": "1.1.1",
    "@abstract-foundation/agw-react": "1.2.3",
    "@privy-io/cross-app-connect": "0.1.5",
    "motion": "^12.6.3",
    "next": "14.2.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.33.5",
    "viem": "2.21.26",
    "wagmi": "2.12.25"
  },
  "scripts": {
    "dev": "pnpm prebuild && next dev",
    "prebuild": "node scripts/set-build-time.js",
    "build": "next build",
    "start": "pnpm prebuild && npx serve@latest out",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "prepare": "husky install",
    "deploy:s3": "next build && bash deploy.sh",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.2.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "20.11.30",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "@vitest/ui": "^3.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.2.4",
    "eslint-plugin-prettier": "5.2.5",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "8.4.35",
    "prettier": "^3.1.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.2",
    "vitest": "^3.1.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
